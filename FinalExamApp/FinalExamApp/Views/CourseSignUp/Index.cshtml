@model SignUpViewModel

<div>
    <a asp-action="Index" asp-controller="Home" class="btn btn-primary m-1 p-1">Home</a>
    <a asp-action="ShowAllCourses" asp-controller="Course" class="btn btn-primary m-1 p-1">Back to all courses</a>
</div>


    @foreach (var m in Model.CourseSignUps)
    {
        <div class="card">
            <img src="" class="card-img-top" />
            <div class="card-body">
                <h5 class="card-title">@m.FullName</h5>
                <p class="card-text">@m.Address</p>
                <p class="card-text">TODO EMAIL</p>
                <a href="#" class="btn btn-primary">TODO EDIT</a>
            </div>
        </div>
    }


    <div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
         page-class="btn" page-class-normal="btn-secondary"
         page-class-selected="btn-primary" class="btn-group pull-right m-1">
    </div>

